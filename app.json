{
  "expo": {
    "name": "networks-qrcode",
    "slug": "networks-qrcode",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images-native/icon.png",
    "scheme": "networks-qrcode",
    "userInterfaceStyle": "automatic",
    "splash": {
      "image": "./assets/images-native/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images-native/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      }
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images-native/favicon.png"
    },
    "plugins": [
      "expo-router",
      "@react-native-firebase/app"
    ],
    "experiments": {
      "typedRoutes": true
    }
  }
}
